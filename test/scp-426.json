        "words": [
          {
            "Bold": {
              "words": [
                {
                  "Text": {
                    "contents": "Date:"
                  }
                }
              ]
            }
          },
          {
            "Text": {
              "contents": " ██-██-████"
            }
          }
        ]
      }
    },
    {
      "Words": {
        "centered": false,
        "words": [
          {
            "Bold": {
              "words": [
                {
                  "Text": {
                    "contents": "Subject:"
                  }
                }
              ]
            }
          },
          {
            "Text": {
              "contents": " D-class personnel D/426/4"
            }
          }
        ]
      }
    },
    {
      "Words": {
        "centered": false,
        "words": [
          {
            "Bold": {
              "words": [
                {
                  "Text": {
                    "contents": "Procedure:"
                  }
                }
              ]
            }
          },
          {
            "Text": {
              "contents": " D/426/4 was placed in isolation in a cell adjacent to my chamber, to be observed until my secondary effect manifests."
            }
          }
        ]
      }
    },
    {
      "Words": {
        "centered": false,
        "words": [
          {
            "Bold": {
              "words": [
                {
                  "Text": {
                    "contents": "Details:"
                  }
                }
              ]
            }
          },
          {
            "Text": {
              "contents": " No effects appeared. D/426/4 was terminated 90 days after the start of the experiment."
            }
          }
        ]
      }
    },
    {
      "Words": {
        "centered": false,
        "words": [
          {
            "Italics": {
              "words": [
                {
                  "Text": {
                    "contents": "Thank God there are some limits to my effects. A lot of us were really starting to get worried about me."
                  }
                }
              ]
            }
          },
          {
            "Text": {
              "contents": " - Dr. C███████"
            }
          }
        ]
      }
    },
